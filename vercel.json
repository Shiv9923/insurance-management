{
  "builds": [
    { "src": "insurancemanagement/wsgi.py", "use": "@vercel/python", "config": { "runtime": "python3.9" } }
  ],
  "buildCommand": "python manage.py collectstatic --noinput",
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "insurancemanagement/wsgi.py" }
  ]
}